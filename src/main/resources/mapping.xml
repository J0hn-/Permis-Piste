<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.polytech.models.Action" table="action" schema="permispiste">
        <id name="numaction" column="NUMACTION"/>
        <property name="actNumaction" column="ACT_NUMACTION"/>
        <property name="libaction" column="LIBACTION"/>
        <property name="scoremin" column="SCOREMIN"/>
        <many-to-one name="actionByActNumaction" class="fr.polytech.models.Action">
            <column name="ACT_NUMACTION" not-null="true"/>
        </many-to-one>
        <set name="actionsByNumaction" inverse="true">
            <key>
                <column name="ACT_NUMACTION" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Action"/>
        </set>
        <set name="appartientsByNumaction" inverse="true">
            <key>
                <column name="NUMACTION"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Appartient"/>
        </set>
        <set name="estAssociesByNumaction" inverse="true">
            <key>
                <column name="NUMACTION"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.EstAssocie"/>
        </set>
        <set name="indicateursByNumaction" inverse="true">
            <key>
                <column name="NUMACTION"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Indicateur"/>
        </set>
        <set name="obtientsByNumaction" inverse="true">
            <key>
                <column name="NUMACTION"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Obtient"/>
        </set>
    </class>
    <class name="fr.polytech.models.Appartient" table="appartient" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.AppartientPK">
            <key-property name="numjeu" column="NUMJEU"/>
            <key-property name="numaction" column="NUMACTION"/>
        </composite-id>
        <many-to-one name="actionByNumaction" class="fr.polytech.models.Action">
            <column name="NUMACTION"/>
        </many-to-one>
        <many-to-one name="jeuByNumjeu" class="fr.polytech.models.Jeu">
            <column name="NUMJEU"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Apprenant" table="apprenant" schema="permispiste">
        <id name="numapprenant" column="NUMAPPRENANT"/>
        <property name="nomapprenant" column="NOMAPPRENANT"/>
        <property name="prenomapprenant" column="PRENOMAPPRENANT"/>
        <set name="inscriptionsByNumapprenant" inverse="true">
            <key>
                <column name="NUMAPPRENANT"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Inscription"/>
        </set>
        <set name="obtientsByNumapprenant" inverse="true">
            <key>
                <column name="NUMAPPRENANT"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Obtient"/>
        </set>
    </class>
    <class name="fr.polytech.models.Calendrier" table="calendrier" schema="permispiste">
        <id name="datejour" column="DATEJOUR"/>
    </class>
    <class name="fr.polytech.models.Comprend" table="comprend" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.ComprendPK">
            <key-property name="numjeu" column="NUMJEU"/>
            <key-property name="nummission" column="NUMMISSION"/>
        </composite-id>
    </class>
    <class name="fr.polytech.models.EstAssocie" table="est_associe" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.EstAssociePK">
            <key-property name="numaction" column="NUMACTION"/>
            <key-property name="numobjectif" column="NUMOBJECTIF"/>
        </composite-id>
        <many-to-one name="actionByNumaction" class="fr.polytech.models.Action">
            <column name="NUMACTION"/>
        </many-to-one>
        <many-to-one name="objectifByNumobjectif" class="fr.polytech.models.Objectif">
            <column name="NUMOBJECTIF"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Fixe" table="fixe" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.FixePK">
            <key-property name="nummission" column="NUMMISSION"/>
            <key-property name="numobjectif" column="NUMOBJECTIF"/>
        </composite-id>
        <many-to-one name="objectifByNumobjectif" class="fr.polytech.models.Objectif">
            <column name="NUMOBJECTIF"/>
        </many-to-one>
        <many-to-one name="missionByNummission" class="fr.polytech.models.Mission">
            <column name="NUMMISSION"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Indicateur" table="indicateur" schema="permispiste">
        <id name="numindic" column="NUMINDIC"/>
        <property name="numaction" column="NUMACTION"/>
        <property name="poids" column="POIDS"/>
        <many-to-one name="actionByNumaction" class="fr.polytech.models.Action">
            <column name="NUMACTION"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Inscription" table="inscription" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.InscriptionPK">
            <key-property name="numapprenant" column="NUMAPPRENANT"/>
            <key-property name="numjeu" column="NUMJEU"/>
        </composite-id>
        <many-to-one name="apprenantByNumapprenant" class="fr.polytech.models.Apprenant">
            <column name="NUMAPPRENANT"/>
        </many-to-one>
        <many-to-one name="jeuByNumjeu" class="fr.polytech.models.Jeu">
            <column name="NUMJEU"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Jeu" table="jeu" schema="permispiste">
        <id name="numjeu" column="NUMJEU"/>
        <property name="libellejeu" column="LIBELLEJEU"/>
        <set name="appartientsByNumjeu" inverse="true">
            <key>
                <column name="NUMJEU"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Appartient"/>
        </set>
        <set name="inscriptionsByNumjeu" inverse="true">
            <key>
                <column name="NUMJEU"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Inscription"/>
        </set>
        <set name="missionsByNumjeu" inverse="true">
            <key>
                <column name="NUMJEU"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Mission"/>
        </set>
    </class>
    <class name="fr.polytech.models.Mission" table="mission" schema="permispiste">
        <id name="nummission" column="NUMMISSION"/>
        <property name="numjeu" column="NUMJEU"/>
        <property name="libmission" column="LIBMISSION"/>
        <many-to-one name="jeuByNumjeu" class="fr.polytech.models.Jeu">
            <column name="NUMJEU"/>
        </many-to-one>
        <set name="fixesByNummission" inverse="true">
            <key>
                <column name="NUMMISSION"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Fixe"/>
        </set>
    </class>
    <class name="fr.polytech.models.Objectif" table="objectif" schema="permispiste">
        <id name="numobjectif" column="NUMOBJECTIF"/>
        <property name="libobectif" column="LIBOBECTIF"/>
        <set name="estAssociesByNumobjectif" inverse="true">
            <key>
                <column name="NUMOBJECTIF"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.EstAssocie"/>
        </set>
        <set name="fixesByNumobjectif" inverse="true">
            <key>
                <column name="NUMOBJECTIF"/>
            </key>
            <one-to-many not-found="ignore" class="fr.polytech.models.Fixe"/>
        </set>
    </class>
    <class name="fr.polytech.models.Obtient" table="obtient" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.ObtientPK">
            <key-property name="numapprenant" column="NUMAPPRENANT"/>
            <key-property name="numaction" column="NUMACTION"/>
        </composite-id>
        <property name="valeur" column="VALEUR"/>
        <many-to-one name="actionByNumaction" class="fr.polytech.models.Action">
            <column name="NUMACTION"/>
        </many-to-one>
        <many-to-one name="apprenantByNumapprenant" class="fr.polytech.models.Apprenant">
            <column name="NUMAPPRENANT"/>
        </many-to-one>
    </class>
    <class name="fr.polytech.models.Possede" table="possede" schema="permispiste">
        <composite-id mapped="true" class="fr.polytech.models.PossedePK">
            <key-property name="numaction" column="NUMACTION"/>
            <key-property name="numregle" column="NUMREGLE"/>
        </composite-id>
    </class>
    <class name="fr.polytech.models.Regle" table="regle" schema="permispiste">
        <id name="numregle" column="NUMREGLE"/>
        <property name="libregle" column="LIBREGLE"/>
        <property name="scoremin" column="SCOREMIN"/>
    </class>
    <class name="fr.polytech.models.Users" table="users" schema="permispiste">
        <id name="id" column="id"/>
        <property name="email" column="email"/>
        <property name="name" column="name"/>
    </class>
</hibernate-mapping>